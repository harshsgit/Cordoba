                    </div>
                    <div id="today-revenue"></div>
                </div>
                <!-- /today's revenue -->
            </div>
            <div class="col-lg-3">
                <!-- Today's revenue -->
                <div class="panel bg-danger-400">
                    <div class="panel-body">
                        <div class="heading-elements">
                            <ul class="icons-list">
                                <li><a data-action=""></a></li>
                            </ul>
                        </div>
                        @*<h3 class="no-margin">$18,390</h3>*@
                        <h3 class="no-margin">{{DashboardSummary.DashboardHeaderSummary.TotalSales}}</h3>
                        Total Sales (in points)
                        <div class="text-muted text-size-small"></div>
                    </div>
                    <div id="today-revenue"></div>
                </div>
                <!-- /today's revenue -->
            </div>

            <!-- /quick stats boxes -->

        </div>
        <div class="row">
            <div class="col-lg-6">
                <!-- Nightingale roses width hidden labels -->
                <div class="panel panel-flat">
                    <div class="panel-heading">
                        <h5 class="panel-title">Order Summary(Current Year)</h5>
                        <div class="heading-elements">
                            @*<ul class="icons-list">
                                    <li><a data-action="collapse"></a></li>
