            <div class="col-lg-6">
                <!-- Basic bar chart -->
                <div class="panel panel-flat">
                    <div class="panel-heading">
                        <h5 class="panel-title">
                            Sales Analytics
                            <span style="float: right;margin-right: 35px;font-size: small;">
                                <select ng-model="ChartFiltertype" class="form-control" ng-change="GetDashboardSummaryCharts(ChartOrFunctionTypeEnum.All)" ng-options="chartfilter.id as chartfilter.name for chartfilter in ChartFilterTypeEnum"></select>
                            </span>
                        </h5>

                        <div class="heading-elements">
                            <ul class="icons-list">
                                @*<li><a data-action="collapse"></a></li>
                                    <li><a data-action="reload"></a></li>
                                    <li><a data-action="close"></a></li>*@
                            </ul>
                        </div>
                    </div>
                    <div class="panel-body">
                        <div class="chart-container">
                            <div class="chart has-fixed-height" id="SalesAnalytics"></div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
        <div class="row">
            <!-- - Top 5 stores Selling -->
            <div class="col-lg-6">

                <div class="panel panel-flat">
                    <div class="panel-heading">
                        <h5 class="panel-title">Top 5 Selling Stores</h5>
                        <div class="heading-elements">
                            @*<ul class="icons-list">
                                    <li><a data-action="collapse"></a></li>
                                    <li><a data-action="reload"></a></li>
                                    <li><a data-action="close"></a></li>
                                </ul>*@
                        </div>
                    </div>
                    <div class="panel-body">
                        <div class="chart-container">
                            <div class="chart has-fixed-height" id="Top5Sales_Chart"></div>
                        </div>
                    </div>
                </div>

            </div>
            <!-- - Top 5 stores Selling -->
            <div class="col-lg-6">
                <!-- Top 5 Customer -->
                <div class="panel panel-flat">
                    <div class="panel-heading">
                        <h5 class="panel-title">Top 5 Customers</h5>
                        <div class="heading-elements">
                            @*<ul class="icons-list">
                                    <li><a data-action="collapse"></a></li>
                                    <li><a data-action="reload"></a></li>
                                    <li><a data-action="close"></a></li>
                                </ul>*@
                        </div>
                    </div>
                    <div class="panel-body">
                        <div class="chart-container">
                            <div class="chart has-fixed-height" id="Top5Customer_Chart"></div>
                        </div>
                    </div>
                </div>
                <!-- - Top 5 stores Selling -->

            </div>
        </div>
        <div class="row">
            <div class="col-lg-4 col-md-12 col-sm-12 col-sx-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h5 class="panel-title"><i class="fa fa-calendar"></i> Recent Activity</h5>
                    </div>
                    <ul class="list-group">
                        <li class="list-group-item" ng-repeat="activity in activityList">
                            <a ng-if="activity.activity_type==1 || activity.activity_type==4" ui-sref="ManageCustomer({CustomerId: {{activity.entitykey}} })">{{activity.activity_text}}</a>
                            <a ng-if="activity.activity_type==2" ui-sref="Orders({OrderId: {{activity.entitykey}} })">{{activity.activity_text}}</a>
                            {{activity.activity_value}} {{activity.comments!=''?activity.comments:''}}.
                            <br>
                            <small class="text-muted"><i class="fa fa-clock-o"></i> {{activity.date_added | date:GlobalDateFormat +'  h:mm a'}}</small>
                        </li>
                        @*<li class="list-group-item">
                                <a>Liam Guinan</a> purchased  ROBERTS ECO PLAY DAB FM RDS DIGITAL RADIO BLACK.<br>
                                <small class="text-muted"><i class="fa fa-clock-o"></i> 03/05/2017 09:20:36</small>
