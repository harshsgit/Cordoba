                            </li>*@
                    </ul>
                </div>
            </div>
            <div class="col-lg-8 col-md-12 col-sm-12 col-sx-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h5 class="panel-title"><i class="fa fa-shopping-cart"></i> Latest Orders</h5>
                    </div>
                    <div class="table-responsive">
                        <table class="table">
                            <thead>
                                <tr>
                                    <td class="text-center">Order ID</td>
                                    <td>Customer</td>
                                    <td>Status</td>
                                    <td>Date Added</td>
                                    <td class="text-center">Total</td>
                                    <td class="text-center">Action</td>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="order in Orders">
                                    <td class="text-center" ng-bind="order.order_id"></td>
                                    <td ng-bind="order.customer"></td>
                                    <td ng-bind="order.OrderStatusName"></td>
                                    <td ng-bind="order.date_added | date:GlobalDateFormat"></td>
                                    <td class="text-center" ng-bind="(order.total | number:0) + ' Points'"></td>
                                    <td class="text-center">
                                        <a href="" class="btn btn-success" ui-sref="Orders({OrderId:order.order_id})" target="_blank" title="view Order details"><span>View</span></a>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>












